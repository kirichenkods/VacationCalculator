    <changeSet id="insert-holiday-dates-1" author="kirichenkods">
        <preConditions onFail="MARK_RAN">
            <sqlCheck expectedResult="0">SELECT COUNT(1)
                                         FROM holidays
                                         WHERE day = 1 AND month = 1;
            </sqlCheck>
        </preConditions>

        <insert schemaName="public" tableName="holidays">
            <column name="month" value="1"/>
            <column name="day" value="1"/>
        </insert>
    </changeSet>

